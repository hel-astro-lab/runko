#!/bin/bash
#SBATCH -A 2019-2-22
#SBATCH -J y8x256
#SBATCH --output=%J.out
#SBATCH --error=%J.err
#SBATCH -t 0-23:00:00
#SBATCH --nodes 128
#SBATCH --ntasks-per-node=32

# activate threading
export OMP_NUM_THREADS=1
export PYTHONDONTWRITEBYTECODE=true
export HDF5_USE_FILE_LOCKING=FALSE

# go to working directory
cd $RUNKODIR/projects/pic3d/

srun -n 4096 python3 pic.py --conf shock.ini
